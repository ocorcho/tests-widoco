@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix estur-core: <https://ontologia.segittur.es/turismo/def/core#> .
@prefix estur-kos: <https://ontologia.segittur.es/turismo/kos/> .

@prefix estur-routeservice: <https://ontologia.segittur.es/turismo/kos/routeservice/> .

#################################################################
#
#    Concept Scheme
#
#################################################################

estur-kos:RouteService a skos:ConceptScheme ; 
    dct:issued "2024-07-23"^^xsd:date ;
    dct:modified "2025-12-15"^^xsd:date ;
    cc:license <https://creativecommons.org/licenses/by/4.0/> ;
    rdfs:comment "This concept scheme belongs to the set of controlled SKOS-based thesauri used by the SEGITTUR tourism ontology."@en ;
    rdfs:comment "Este esquema de conceptos pertenece al conjunto de tesauros SKOS usados por la ontología de turismo de SEGITTUR."@es ;
    rdfs:label "Route service"@en ;
    rdfs:label "Servicio asociado a una ruta"@es ;
    dct:title "Concept scheme grouping route services"@en ;
    dct:title "Esquema de conceptos agrupando servicios asociados a una ruta"@es .

#################################################################
#
#    Concepts
#
#################################################################

estur-routeservice:Signaling a skos:Concept;
  skos:inScheme estur-kos:RouteService;
  skos:prefLabel "Signaling"@en ;
  skos:prefLabel "Señalización"@es .

estur-routeservice:Lighting a skos:Concept;
  skos:inScheme estur-kos:RouteService;
  skos:prefLabel "Ligthing"@en ;
  skos:prefLabel "Alumbrado"@es .

estur-routeservice:AccessibleSignage a skos:Concept;
  skos:inScheme estur-kos:RouteService;
  skos:prefLabel "Accessible signage"@en ;
  skos:prefLabel "Señalética accesible"@es .
