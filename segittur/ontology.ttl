        rdfs:subClassOf :NaturalResource ;
        rdfs:comment "Llanura entre montes o alturas"@es ,
                     "Plain between mountains or heights"@en ;
        rdfs:label "valle"@es ,
                   "valley"@en ;
        <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


###  https://ontologia.segittur.es/turismo/def/core#VehicleExperience
:VehicleExperience rdf:type owl:Class ;
                   rdfs:subClassOf :LeisureAndCultureFacility ;
                   rdfs:comment "Pequeñas excursiones en tren, bus o barco para conocer los principales monumentos y atracciones de una ciudad."@es ,
                                "Small excursions by train, bus or boat to see the main monuments and attractions of a city."@en ;
                   rdfs:label "experiencia en vehículo"@es ,
                              "vehicle experience"@en ;
                   <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


