        rdfs:subClassOf :AgrotourismFacility ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :offersAdditionalWineryService ;
                          owl:allValuesFrom <http://www.w3.org/2004/02/skos/core#Concept>
                        ] ;
        rdfs:comment "Lugar donde se cría y almacena el vino."@es ,
                     "Place where wine is raised and stored."@en ;
        rdfs:label "bodega"@es ,
                   "winery"@en ;
        <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


###  https://ontologia.segittur.es/turismo/def/core#WinterSportsResort
:WinterSportsResort rdf:type owl:Class ;
                    rdfs:subClassOf :SportFacility ;
                    rdfs:comment "Centro turístico básicamente dedicado a la práctica del esquí y demás deportes de nieve y montaña, que forma un conjunto coordinado de medios de remonte mecánicos, pistas e instalaciones complementarias."@es ,
                                 "Tourist center basically dedicated to the practice of skiing and other snow and mountain sports, which forms a coordinated set of mechanical lifts, slopes and complementary facilities."@en ;
                    rdfs:label "estación de deportes de invierno"@es ,
                               "winter sports resort"@en ;
                    <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


###  https://ontologia.segittur.es/turismo/def/core#YatchingPort
:YatchingPort rdf:type owl:Class ;
              rdfs:subClassOf :SportFacility ;
              rdfs:comment "Instalación ubicada en una zona costera o junto a un cuerpo de agua, que se utiliza para el amarre y resguardo de embarcaciones de recreo y deportivas, como yates, veleros, lanchas y otros tipos de barcos privados."@es ,
                           "Installation located in a coastal area or next to a body of water, which is used for mooring and sheltering recreational and sports vessels, such as yachts, sailboats, motorboats and other types of private boats."@en ;
              rdfs:label "puerto deportivo"@es ,
                         "yachting port"@en ;
              <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


###  https://ontologia.segittur.es/turismo/def/core#YogaCentre
:YogaCentre rdf:type owl:Class ;
            rdfs:subClassOf :HealthAndWellnessFacility ;
            rdfs:comment "Establecimiento en el que se practica yoga"@es ,
                         "Establishment where yoga is practiced"@en ;
            rdfs:label "Centro de yoga"@es ,
                       "Yoga Centre"@en ;
            <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


###  https://ontologia.segittur.es/turismo/def/core#Zoo
:Zoo rdf:type owl:Class ;
     rdfs:subClassOf :LeisureAndCultureFacility ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :offersAdditionalZooService ;
                       owl:allValuesFrom <http://www.w3.org/2004/02/skos/core#Concept>
                     ] ;
     rdfs:comment "Lugar en que se conservan, cuidan y a veces se crían diversas especies animales para que sean contempladas por el público y para su estudio."@es ,
                  "Place where various animal species are conserved, cared for and sometimes raised so that they can be seen by the public and for study."@en ;
     rdfs:label "zoo"@en ,
                "zoológico"@es ;
     <https://ontologia.segittur.es/turismo/def/annot#visibleClass> "true"^^xsd:boolean .


