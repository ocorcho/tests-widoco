#################################################################

###  https://ontology.tno.nl/logistics/federated/BusinessService#LegalPerson
<https://ontology.tno.nl/logistics/federated/BusinessService#LegalPerson> rdf:type owl:Class .


###  https://ontology.tno.nl/logistics/federated/DigitalTwin#TransportMeans
<https://ontology.tno.nl/logistics/federated/DigitalTwin#TransportMeans> rdf:type owl:Class .


###  https://ontology.tno.nl/logistics/federated/Event#Event
<https://ontology.tno.nl/logistics/federated/Event#Event> rdf:type owl:Class .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles> rdf:type owl:Class ;
                                                                       rdfs:subClassOf owl:Thing ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <https://ontology.tno.nl/logistics/federated/LogisticRoles#isAbout> ;
                                                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                         owl:onClass <https://ontology.tno.nl/logistics/federated/BusinessService#LegalPerson>
                                                                                       ] ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :isAbout ;
                                                                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                         owl:onClass <https://ontology.tno.nl/logistics/federated/BusinessService#LegalPerson>
                                                                                       ] ;
                                                                       rdfs:comment "An Enterprise, Person or Organization can have different roles depending on the context. This class defines these different roles." ;
                                                                       rdfs:label "Roles" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#BusinessRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#BusinessRoles> rdf:type owl:Class ;
                                                                          rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles> ;
                                                                          rdfs:comment "Logistics roles that describe the different functions of companies regarding services." ;
                                                                          rdfs:label "Business Roles" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#CommercialRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#CommercialRoles> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles> ;
                                                                            rdfs:comment "Logistics roles that describe the different commercial functions of companies." ;
                                                                            rdfs:label "Commercial Roles" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#DropOffLocation
<https://ontology.tno.nl/logistics/federated/LogisticRoles#DropOffLocation> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge> ;
                                                                            rdfs:comment "A place where cargo on a transport means was dropped off. Modality 'road'." ;
                                                                            rdfs:label "Drop Off Location Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#FinancialRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#FinancialRoles> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles> ;
                                                                           rdfs:comment "Logistics roles that describe the different financial functions of companies." ;
                                                                           rdfs:label "Financial Roles" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> rdf:type owl:Class ;
                                                                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <https://ontology.tno.nl/logistics/federated/LogisticRoles#involves> ;
                                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                            owl:onClass <https://ontology.tno.nl/logistics/federated/DigitalTwin#TransportMeans>
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <https://ontology.tno.nl/logistics/federated/LogisticRoles#involves> ;
                                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                            owl:onClass <https://ontology.tno.nl/logistics/federated/Event#Event>
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <https://ontology.tno.nl/logistics/federated/LogisticRoles#involves> ;
                                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                            owl:onClass <https://ontology.tno.nl/logistics/federated/PhysicalInfrastructure#Location>
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty :involves ;
                                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                            owl:onClass <https://ontology.tno.nl/logistics/federated/DigitalTwin#TransportMeans>
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty :involves ;
                                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                            owl:onClass <https://ontology.tno.nl/logistics/federated/Event#Event>
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty :involves ;
                                                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                            owl:onClass <https://ontology.tno.nl/logistics/federated/PhysicalInfrastructure#Location>
                                                                                          ] ;
                                                                          rdfs:comment "A location such as hub or port can have different roles depending on the context. This class defines these different roles." ;
                                                                          rdfs:label "Location Roles" ;
                                                                          <https://ontology.tno.nl/logistics/federated/ReusableTags#isDerived> "true"^^xsd:boolean .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticServiceProvider
<https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticServiceProvider> rdf:type owl:Class .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticsRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticsRoles> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles> ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <https://ontology.tno.nl/logistics/federated/BusinessService#logisticsRoleName> ;
                                                                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                             owl:onDataRange xsd:string
                                                                                           ] ;
                                                                           rdfs:comment "Logistics roles that describe the different functions of companies during transportation." ;
                                                                           rdfs:label "Logistics Roles" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PickupLocation
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PickupLocation> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading> ;
                                                                           rdfs:comment "A location where cargo is picked up by a transport means." ;
                                                                           rdfs:label "Pickup Location Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfAcceptance
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfAcceptance> rdf:type owl:Class ;
                                                                              rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                              rdfs:comment "Place of acceptance of a product." ;
                                                                              rdfs:label "Place of Acceptance Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfArrival
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfArrival> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                           rdfs:comment "A place of a call of a transport means following (in time) the last place of departure (milestone value is departure)" ;
                                                                           rdfs:label "Arrival Location Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfBorderCrossing
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfBorderCrossing> rdf:type owl:Class ;
                                                                                  rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                                  rdfs:comment "A place where a new customs domain is entered by a transport means during its itinerary." ;
                                                                                  rdfs:label "Border Crossing Place Role" ;
                                                                                  <https://ontology.tno.nl/logistics/federated/ReusableTags#isDerived> "true"^^xsd:boolean .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfCall
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfCall> rdf:type owl:Class ;
                                                                        rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                        rdfs:comment "A place where a transport means stops on a voyage." ;
                                                                        rdfs:label "Place of Call Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDelivery
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDelivery> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                            rdfs:comment "A place of receipt by a consignee or place of delivery is the place where the cargo is handed over by a carrier according a business transaction to the recipeint (consignee). The transport means and the cargo have to be at the location mentioned in a business transaction between a carrier and its customer." ;
                                                                            rdfs:label "Place of Delivery Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDeparture
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDeparture> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                             rdfs:comment "A place where a transport means arrived (milestone value: arrival) and is able to depart." ,
                                                                                          "The first place of call of a transport means or a place of call of a transport means following arrival." ;
                                                                             rdfs:label "Departure Location Role" ,
                                                                                        "Place of Departure Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge> rdf:type owl:Class ;
                                                                             owl:equivalentClass <https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfUnloading> ;
                                                                             rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                             rdfs:comment "For modality 'road'. Same as place of discharge." ;
                                                                             rdfs:label "Inland Terminal" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                           rdfs:comment "A place of loading is the place where the cargo is offered for transport by a business transaction to the service provider that is the carrier, where the transport means is considered to be the main modality. The cargo and the transport means have to be at that location." ;
                                                                           rdfs:label "Place of Loading Role" ;
                                                                           <https://ontology.tno.nl/logistics/federated/ReusableTags#isDerived> "true"^^xsd:boolean .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfReceipt
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfReceipt> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                           rdfs:comment "A place of receipt is the place where the cargo is offered for transport by a business transaction to the service provider that is the carrier and has to be transported by that carrier to the place of loading of the main transport. The cargo has to be at that location." ;
                                                                           rdfs:label "Place of Receipt" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStripping
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStripping> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                             rdfs:comment "A place where goods is unloaded from a container." ;
                                                                             rdfs:label "Place of Stripping Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStuffing
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStuffing> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                            rdfs:comment "A place where goods is loaded into an empty container which is then sealed and transported to the carrier for loading on a transport means." ;
                                                                            rdfs:label "Place of Stuffing Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfUnloading
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfUnloading> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles> ;
                                                                             rdfs:comment "A place of unloading is the place to where the cargo has to be transported by a business transaction to the service provider that is the carrier and will be discharged/unloaded from that transport means. The cargo and the transport means have to be at that location." ;
                                                                             rdfs:label "Place of Unloading Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfCall
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfCall> rdf:type owl:Class ;
                                                                       rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfCall> ;
                                                                       rdfs:comment "A place where a vessel stops on a voyage. The necessary context is a location and transport means with modality 'sea'." ;
                                                                       rdfs:label "Port of Call Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfDischarge
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfDischarge> rdf:type owl:Class ;
                                                                            rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge> ;
                                                                            rdfs:comment "A place where a vessel (partially) discharges or unloads its shipments." ;
                                                                            rdfs:label "Port of Discharge Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfLoading
<https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfLoading> rdf:type owl:Class ;
                                                                          rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading> ;
                                                                          rdfs:comment "A port where shipments are loaded aboard a vessel for transport." ;
                                                                          rdfs:label "Port of Loading Role" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#SupplyChainRoles
<https://ontology.tno.nl/logistics/federated/LogisticRoles#SupplyChainRoles> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles> ;
                                                                             rdfs:comment "These are roles that are found elsewhere in the supply chain than described by bussiness, commercial, financial or logistical roles. Such as the consumer or producer of products." ;
                                                                             rdfs:label "Supply Chain Roles" .


###  https://ontology.tno.nl/logistics/federated/PhysicalInfrastructure#Location
<https://ontology.tno.nl/logistics/federated/PhysicalInfrastructure#Location> rdf:type owl:Class .


###  https://vocab.plataformasimple.es/def/logistics/federated/LogisticRoles#Driver
:Driver rdf:type owl:Class ;
        rdfs:subClassOf <https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticServiceProvider> ;
        rdfs:comment "Persona que conduce un vehículo"@es ,
                     "Someone who drives a vehicle."@en ;
        rdfs:label "Conductor"@es ,
                   "Driver"@en .


#################################################################
#    Individuals
#################################################################

###  https://ontology.tno.nl/logistics/federated/LogisticRoles#Authority
<https://ontology.tno.nl/logistics/federated/LogisticRoles#Authority> rdf:type owl:NamedIndividual ,
                                                                               <https://ontology.tno.nl/logistics/federated/LogisticRoles#CommercialRoles> ;
                                                                      <http://www.w3.org/2004/02/skos/core#narrower> <https://ontology.tno.nl/logistics/federated/LogisticRoles#CustomsAuthority> ,
                                                                                                                     <https://ontology.tno.nl/logistics/federated/LogisticRoles#PortAuthority> ;
                                                                      rdfs:comment "Any public organization responsible for governing one or more regulations and/or maintenance and capacity optimization of public infrastructure can be identified as an Authority. Capacity optimization of public infrastructure can also be expressed by access restrictions, e.g. city distribution restrictions in city centers, expressed by time windows and types of vehicles that are allowed to access. As such, municipalities are also in scope of FEDeRATED." ;
                                                                      rdfs:label "Authority" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#Bank
<https://ontology.tno.nl/logistics/federated/LogisticRoles#Bank> rdf:type owl:NamedIndividual ,
                                                                          <https://ontology.tno.nl/logistics/federated/LogisticRoles#FinancialRoles> ;
                                                                 rdfs:comment "A financial institution licensed to receive deposits and make loans." ;
                                                                 rdfs:label "Bank" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#Carrier
<https://ontology.tno.nl/logistics/federated/LogisticRoles#Carrier> rdf:type owl:NamedIndividual ,
                                                                             <https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticsRoles> ;
                                                                    rdfs:comment "The organization that is responsible for transport of goods/containers with a transport means (truck, vessel, etc.). Each modality has a different name per modality: airline (air), shipping line (sea), barge operator (inlande waterways), railway undertaking (rail), and carrier (road). These should also be supported by the model" ;
                                                                    rdfs:label "Carrier" .


###  https://ontology.tno.nl/logistics/federated/LogisticRoles#Consignee
<https://ontology.tno.nl/logistics/federated/LogisticRoles#Consignee> rdf:type owl:NamedIndividual ,
                                                                               <https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticsRoles> ;
                                                                      rdfs:comment """A consignee is the role of an enterprise where it is the operator and/or owner of a logistical function at a place with the role 'place of delivery'. A consignee can act as customer (e.g. Terms of Delivery is 'ex works').  
defintions 2: A consignee is the role of an enterprise where it is the operator and/or owner of a logistical function at a place with the role 'place of delivery'. A consignee can act as customer (e.g. Terms of Delivery is 'ex works').""" ;
                                                                      rdfs:label "Consignee" ;
