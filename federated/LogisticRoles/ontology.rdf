    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#isAbout"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/BusinessService#LegalPerson"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.plataformasimple.es/def/logistics/federated/LogisticRoles#isAbout"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/BusinessService#LegalPerson"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An Enterprise, Person or Organization can have different roles depending on the context. This class defines these different roles.</rdfs:comment>
        <rdfs:label>Roles</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#BusinessRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#BusinessRoles">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles"/>
        <rdfs:comment>Logistics roles that describe the different functions of companies regarding services.</rdfs:comment>
        <rdfs:label>Business Roles</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#CommercialRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#CommercialRoles">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles"/>
        <rdfs:comment>Logistics roles that describe the different commercial functions of companies.</rdfs:comment>
        <rdfs:label>Commercial Roles</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#DropOffLocation -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#DropOffLocation">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge"/>
        <rdfs:comment>A place where cargo on a transport means was dropped off. Modality &apos;road&apos;.</rdfs:comment>
        <rdfs:label>Drop Off Location Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#FinancialRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#FinancialRoles">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles"/>
        <rdfs:comment>Logistics roles that describe the different financial functions of companies.</rdfs:comment>
        <rdfs:label>Financial Roles</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#involves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/DigitalTwin#TransportMeans"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#involves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/Event#Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#involves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/PhysicalInfrastructure#Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.plataformasimple.es/def/logistics/federated/LogisticRoles#involves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/DigitalTwin#TransportMeans"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.plataformasimple.es/def/logistics/federated/LogisticRoles#involves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/Event#Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.plataformasimple.es/def/logistics/federated/LogisticRoles#involves"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.tno.nl/logistics/federated/PhysicalInfrastructure#Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A location such as hub or port can have different roles depending on the context. This class defines these different roles.</rdfs:comment>
        <rdfs:label>Location Roles</rdfs:label>
        <ReusableTags:isDerived rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ReusableTags:isDerived>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticServiceProvider -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticServiceProvider"/>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticsRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#LogisticsRoles">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#ActorRoles"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.tno.nl/logistics/federated/BusinessService#logisticsRoleName"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Logistics roles that describe the different functions of companies during transportation.</rdfs:comment>
        <rdfs:label>Logistics Roles</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PickupLocation -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PickupLocation">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading"/>
        <rdfs:comment>A location where cargo is picked up by a transport means.</rdfs:comment>
        <rdfs:label>Pickup Location Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfAcceptance -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfAcceptance">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>Place of acceptance of a product.</rdfs:comment>
        <rdfs:label>Place of Acceptance Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfArrival -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfArrival">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place of a call of a transport means following (in time) the last place of departure (milestone value is departure)</rdfs:comment>
        <rdfs:label>Arrival Location Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfBorderCrossing -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfBorderCrossing">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place where a new customs domain is entered by a transport means during its itinerary.</rdfs:comment>
        <rdfs:label>Border Crossing Place Role</rdfs:label>
        <ReusableTags:isDerived rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ReusableTags:isDerived>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfCall -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfCall">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place where a transport means stops on a voyage.</rdfs:comment>
        <rdfs:label>Place of Call Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDelivery -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDelivery">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place of receipt by a consignee or place of delivery is the place where the cargo is handed over by a carrier according a business transaction to the recipeint (consignee). The transport means and the cargo have to be at the location mentioned in a business transaction between a carrier and its customer.</rdfs:comment>
        <rdfs:label>Place of Delivery Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDeparture -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDeparture">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place where a transport means arrived (milestone value: arrival) and is able to depart.</rdfs:comment>
        <rdfs:comment>The first place of call of a transport means or a place of call of a transport means following arrival.</rdfs:comment>
        <rdfs:label>Departure Location Role</rdfs:label>
        <rdfs:label>Place of Departure Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge">
        <owl:equivalentClass rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfUnloading"/>
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>For modality &apos;road&apos;. Same as place of discharge.</rdfs:comment>
        <rdfs:label>Inland Terminal</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place of loading is the place where the cargo is offered for transport by a business transaction to the service provider that is the carrier, where the transport means is considered to be the main modality. The cargo and the transport means have to be at that location.</rdfs:comment>
        <rdfs:label>Place of Loading Role</rdfs:label>
        <ReusableTags:isDerived rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ReusableTags:isDerived>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfReceipt -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfReceipt">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place of receipt is the place where the cargo is offered for transport by a business transaction to the service provider that is the carrier and has to be transported by that carrier to the place of loading of the main transport. The cargo has to be at that location.</rdfs:comment>
        <rdfs:label>Place of Receipt</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStripping -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStripping">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place where goods is unloaded from a container.</rdfs:comment>
        <rdfs:label>Place of Stripping Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStuffing -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfStuffing">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place where goods is loaded into an empty container which is then sealed and transported to the carrier for loading on a transport means.</rdfs:comment>
        <rdfs:label>Place of Stuffing Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfUnloading -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfUnloading">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#LocationRoles"/>
        <rdfs:comment>A place of unloading is the place to where the cargo has to be transported by a business transaction to the service provider that is the carrier and will be discharged/unloaded from that transport means. The cargo and the transport means have to be at that location.</rdfs:comment>
        <rdfs:label>Place of Unloading Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfCall -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfCall">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfCall"/>
        <rdfs:comment>A place where a vessel stops on a voyage. The necessary context is a location and transport means with modality &apos;sea&apos;.</rdfs:comment>
        <rdfs:label>Port of Call Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfDischarge -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfDischarge">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfDischarge"/>
        <rdfs:comment>A place where a vessel (partially) discharges or unloads its shipments.</rdfs:comment>
        <rdfs:label>Port of Discharge Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfLoading -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#PortOfLoading">
        <rdfs:subClassOf rdf:resource="https://ontology.tno.nl/logistics/federated/LogisticRoles#PlaceOfLoading"/>
        <rdfs:comment>A port where shipments are loaded aboard a vessel for transport.</rdfs:comment>
        <rdfs:label>Port of Loading Role</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.tno.nl/logistics/federated/LogisticRoles#SupplyChainRoles -->


    <owl:Class rdf:about="https://ontology.tno.nl/logistics/federated/LogisticRoles#SupplyChainRoles">
